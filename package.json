{
  "name": "@vtian/vtools",
  "version": "2.0.0",
  "description": "tools",
  "main": "dist/vtools.umd.js",
  "module": "dist/vtools.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiandashu/loan-calculate-utils.git"
  },
  "bugs": {
    "url": "https://github.com/tiandashu/loan-calculate-utils/issues"
  },
  "homepage": "https://github.com/tiandashu/loan-calculate-utils#readme",
  "scripts": {
    "dev": "rollup -w --environment NODE_ENV:development -c scripts/rollup.config.dev.js",
    "build": "rollup --environment NODE_ENV:development -c scripts/rollup.config.prod.js",
    "x": "npm --no-git-tag-version version major",
    "y": "npm --no-git-tag-version version minor",
    "z": "npm --no-git-tag-version version patch",
    "lint": "eslint src",
    "fix": "npm run lint --fix",
    "commit": "git-cz",
    "test": "jest",
    "test:c": "jest --coverage",
    "prepublish": "npm run build && npm run x",
    "pub": "npm publish --access=public"
  },
  "author": "tianlu",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-html": "^0.2.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "rollup": "^2.38.5",
    "rollup-jest": "^1.1.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {},
  "jest": {
    "preset": "rollup-jest"
  }
}
